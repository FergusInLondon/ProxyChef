{
  "name": "ProxyChef",
  "version": "0.0.1",
  "description": "Simple scriptable proxy for intercepting, modifying, and logging requests/responses.",
  "main": "main.js",
  "scripts": {
    "run": "node main.js",
    "develop": "nodemon main.js",
    "lint": "eslint ./lib/**",
    "unit": "mocha --recursive './lib/**/*.test.js'",
    "test": "jasmine --config=jasmine.json"
  },
  "keywords": [
    "mitm",
    "proxy",
    "http",
    "https",
    "cyberchef"
  ],
  "author": "Fergus In London",
  "license": "MIT",
  "dependencies": {
    "cyberchef": "^9.20.3",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "http-proxy": "^1.18.0",
    "mocha": "^10.2.0",
    "pino": "^6.2.1",
    "pino-pretty": "^4.0.0",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "jasmine": "^3.5.0",
    "nodemon": "^2.0.3"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "rules": {}
  }
}
